<tr>
  <td class="col-min">
    <span class="label label-success"><i class="fa fa-random"></i></span>
  </td>
  <td>
    <strong>{{ audit_rename(log.source.typLabel) }}</strong> {{ audit_assoc(log.source) }} was associated with

    <strong>{{ audit_rename(log.target.typLabel) }}</strong> {{ audit_assoc(log.target) }},
    {{ audit_blame(log.blame) }}
  </td>
  <td class="col-min">{{ log.loggedAt|time_diff }}</td>
  <td class="col-min"></td>
</tr>
