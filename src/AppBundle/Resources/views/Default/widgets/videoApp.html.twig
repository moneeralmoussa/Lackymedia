<div class="box box-primary">
  <div class="box-header">
    <i class="fa fa-video-camera"></i>
    <h3 class="box-title">VideoApp</h3>
</div>
  <div class="callout callout-primary ">
  <center>
   {% if is_granted('ROLE_ADMIN') or is_granted('ROLE_PERSONAL') or is_granted('ROLE_DISPOSITION') %}
        <video id="video" controls preload="metadata" width="100%">
          <source src="video/Disponent.mp4" type="video/mp4">
          <source src="video/sintel-short.webm" type="video/webm">
          <track label="Deutsch" kind="subtitles" srclang="en" src="video/Disponent.vtt" default>
        </video>
   {% endif %}
 </center> 
  </div>
   <div class="slimScrollBar" style="background: rgb(0, 0, 0) none repeat scroll 0% 0%; width: 7px; position: absolute; top: 63px; opacity: 0.4; display: none; border-radius: 7px; z-index: 99; right: 1px; height: 187.126px;"></div>
    <div class="slimScrollRail" style="width: 7px; height: 100%; position: absolute; top: 0px; display: none; border-radius: 7px; background: rgb(51, 51, 51) none repeat scroll 0% 0%; opacity: 0.2; z-index: 90; right: 1px;"></div>
  </div>
